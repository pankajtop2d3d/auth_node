<div class="container-fluid">
	<h1 class="h3 mb-4 text-gray-800">Edit Profile</h1>
	<div class="row">
		<div class="col-lg-6">
			<div class="card mb-6">
				<div class="card-header">
				</div>
				<div class="card-body">
					<div class="card border-left-success shadow h-10 success_msg">
						<div class="card-body">
							<span id="success_txt" ></span>
						</div>
					 </div>
					 <div class="card border-left-danger shadow h-10 error_msg">
						<div class="card-body">
							<span id="error_txt" ></span>
						</div>
					 </div>
					<form class="form-horizontal" id="profile" name="profile" method="post">
						<div class="form-group">
						  <div class="col-sm-10">
							<input type="text" class="form-control" id="first_name" value="<%- data.user_data.first_name %>" placeholder="Enter First Name" name="first_name">
						  </div>
						</div>
						<div class="form-group">
					
						  <div class="col-sm-10">          
							<input type="text" class="form-control" id="last_name" value="<%- data.user_data.last_name %>" placeholder="Enter Last Name" name="last_name">
						  </div>
						</div>
						<div class="form-group">
							<div class="col-sm-10">          
							  <input type="email" class="form-control" id="email" value="<%- data.user_data.email %>"  placeholder="Enter Email" name="email" readonly>
							</div>
						</div>
						<div class="form-group">        
						  <div class="col-sm-offset-2 col-sm-10">
	
							<input type="submit" value="Edit" class="btn btn-primary btn-user btn-block">
						  </div>
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>

</div>